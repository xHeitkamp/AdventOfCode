{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"rootDir": "./",
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true
	},
	"include": ["*"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
